<tr class="info-tag">
  <td>
    {{#if icon}}
    <i class="{{icon}}"></i>
    {{/if}}
  </td>
  <td>
  {{#if url}}
    <h6 class="info-text">
      <a href="{{url}}" target="_blank">
        {{removeProtocol text}} <i class="fa fa-external-link" aria-hidden="true"></i>
      </a>
    </h6>
    {{else}}
    <h6 class="info-text">{{{text}}}</h6>
    {{/if}}
  </td>
</tr>
